<div class="container">
  <h1 class="text-center m-3"><%= @user.first_name %>'s shortlist</h1>
  <div class="shortlist-container">
    <% @shortlist.each do |booking| %>
      <%= render partial: "shared/list_card", locals: { booking: booking } %>
      <div><%#= link_to "Open chat", venue_booking_chatroom_path(booking.venue, booking, ) %></div>
    <% end %>
  </div>
</div>
